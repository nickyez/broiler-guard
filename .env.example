MQTT_HOST="broker.emqx.io"
MQTT_PORT=8883
MQTT_CLIENT_ID="mqtt-broiler-guard"
MQTT_AUTH_USERNAME=
MQTT_AUTH_PASSWORD=
MQTT_TLS_ENABLED=true
MQTT_TLS_CA_FILE="storage/app/emqxsl-ca.crt"
