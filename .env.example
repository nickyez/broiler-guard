MQTT_TLS_ALLOW_SELF_SIGNED_CERT=true
MQTT_CLEAN_SESSION=false
MQTT_CONNECT_TIMEOUT=10000
MQTT_SOCKET_TIMEOUT=10000
MQTT_RESEND_TIMEOUT=10000
MQTT_AUTO_RECONNECT_MAX_RECONNECT_ATTEMPTS=5